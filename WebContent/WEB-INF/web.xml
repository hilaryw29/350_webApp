	</servlet>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/LoginController</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>UserLoginController</display-name>
		<servlet-name>UserLoginController</servlet-name>
		<servlet-class>com.proj.controller.UserLoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserLoginController</servlet-name>
		<url-pattern>/UserLoginController</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>AdminLoginController</display-name>
		<servlet-name>AdminLoginController</servlet-name>
		<servlet-class>com.proj.controller.AdminLoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminLoginController</servlet-name>
		<url-pattern>/AdminLoginController</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>UserController</display-name>
		<servlet-name>UserController</servlet-name>
		<servlet-class>com.proj.controller.UserController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserController</servlet-name>
		<url-pattern>/UserController</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>ListingController</display-name>
		<servlet-name>ListingController</servlet-name>
		<servlet-class>com.proj.controller.ListingController</servlet-class>
		<multipart-config>
  		<max-file-size>3145728</max-file-size>
 		<max-request-size>5242880</max-request-size>
 </multipart-config>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListingController</servlet-name>
		<url-pattern>/ListingController</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>DisplayImage</display-name>
		<servlet-name>DisplayImage</servlet-name>
		<servlet-class>com.proj.controller.DisplayImage</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DisplayImage</servlet-name>
		<url-pattern>/listingImages/*</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>LogoutServlet</display-name>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.mie.controller.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
